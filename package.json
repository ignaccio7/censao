{
  "name": "censao",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint .",
    "format": "prettier --check .",
    "lint:fix": "oxlint --fix .",
    "format:fix": "prettier --write .",
    "validate": "npm run lint && npm run format",
    "prisma:setup": "npx prisma migrate reset && npx prisma generate",
    "seed": "npx prisma db seed",
    "seed:force": "npx prisma migrate reset --force",
    "prisma:pull": "npx prisma db pull && npx prisma generate",
    "prisma:push": "npx prisma db push && npx prisma generate",
    "prisma:generate": "npx prisma generate",
    "migrate:create": "npx prisma migrate dev",
    "migrate:apply": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:reset": "npx prisma migrate reset",
    "db:sync": "npm run prisma:pull && npm run seed",
    "db:fresh": "npx prisma db push --force-reset && npx prisma generate && npx prisma db seed",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "commit": "cz"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fullcalendar/core": "6.1.19",
    "@fullcalendar/daygrid": "6.1.19",
    "@fullcalendar/interaction": "6.1.19",
    "@fullcalendar/react": "6.1.19",
    "@fullcalendar/timegrid": "6.1.19",
    "@hookform/resolvers": "5.2.1",
    "@prisma/client": "6.15.0",
    "@tanstack/react-query": "5.87.1",
    "axios": "1.11.0",
    "bcryptjs": "3.0.2",
    "next": "^15.5.9",
    "next-auth": "5.0.0-beta.29",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "7.62.0",
    "sonner": "^2.0.7",
    "zod": "4.0.17",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@prettier/plugin-oxc": "0.0.4",
    "@tailwindcss/postcss": "4",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "oxlint": "1.12.0",
    "prettier": "3.6.2",
    "prisma": "6.15.0",
    "tailwindcss": "4",
    "tsx": "4.20.5",
    "typescript": "5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
